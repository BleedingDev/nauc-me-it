{
  "name": "tanstack-transcribe",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tanstack-transcribe/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project tanstack-transcribe --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/tanstack-transcribe",
        "commands": ["vinxi build"],
        "forwardAllArgs": false
      },
      "cache": true
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/tanstack-transcribe",
        "commands": ["vinxi start"],
        "forwardAllArgs": false
      },
      "cache": false
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/tanstack-transcribe",
        "commands": ["vinxi dev"],
        "forwardAllArgs": false
      },
      "cache": false
    }
  }
}
