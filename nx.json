{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": ["default"],
    "sharedGlobals": []
  },
  "plugins": [
    {
      "plugin": "@nx/next/plugin",
      "options": {
        "startTargetName": "start",
        "buildTargetName": "build",
        "devTargetName": "dev",
        "serveStaticTargetName": "serve-static"
      }
    }
  ],
  "generators": {
    "@nx/next": {
      "application": {
        "style": "tailwind",
        "linter": "none"
      }
    },
    "@nx/react": {
      "library": {
        "style": "tailwind",
        "linter": "none",
        "unitTestRunner": "none"
      },
      "application": {
        "babel": true,
        "style": "tailwind",
        "bundler": "rspack"
      },
      "component": {
        "style": "tailwind"
      }
    }
  },
  "nxCloudAccessToken": "OWJlNGIzNzctMTJjMi00MjdkLThiYWItNzRmZDBiM2ZkM2JkfHJlYWQtd3JpdGU=",
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "@nx/webpack:webpack": {
      "inputs": [
        "production",
        "^production",
        {
          "env": "NX_MF_DEV_REMOTES"
        }
      ],
      "dependsOn": ["^build"],
      "cache": true
    }
  }
}
