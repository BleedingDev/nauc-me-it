        "project": "./tsconfig.json"
    },
    "plugins": ["@typescript-eslint", "functional"],
    "extends": ["next/core-web-vitals", "prettier", "plugin:functional/lite"],
    "rules": {
        "functional/no-return-void": ["error", { "ignoreImplicit": true }]
    }
}
